<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Tables.DynamicTableView">
   <center>
      <TableView fx:id="tableView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" />
   </center>
   <top>
      <Label fx:id="table_title" BorderPane.alignment="CENTER">
         <font>
            <Font name="Baskerville Old Face" size="14.0" />
         </font></Label>
   </top>
</BorderPane>
